{% macro render(section) %}
    <div class="inner" id="{{ section.id }}">
        <section>
            <div class="inner">
                <div class="row">
                    {% set ordered = ordered_blocks(section.blocks) %}
                    {% for block in ordered %}
                        <div class="col-4 col-12-medium">
                            {% import '_macros/_builder/_blocks/' ~ block.module ~ '.html.twig' as module %}
                            {{ module.render(block.content) }}
                        </div>
                    {% endfor %}
                </div>
            </div>
        </section>
    </div>
{% endmacro %}